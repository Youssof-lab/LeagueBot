{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "requireConfigFile": false
  },
  "rules": {
    "no-console": "off",
    "func-names": "off",
    "linebreak-style": "off",
    "no-use-before-define": "off",
    "no-plusplus": "off",
    "import/no-extraneous-dependencies": "off",
    "import/no-dynamic-require": "off",
    "function-paren-newline": "off",
    "no-unused-vars": "warn",
    "quotes": [
      2,
      "double",
      {
        "avoidEscape": true
      }
    ],
    "max-len": [
      "error",
      {
        "code": 120
      }
    ],
    "class-methods-use-this": [
      "error",
      {
        "exceptMethods": [
          "run",
          "buildSlashCommand",
          "runSlash"
        ]
      }
    ],
    "implicit-arrow-linebreak": [
      "error",
      {
        "beside": true
      }
    ],
  }
}